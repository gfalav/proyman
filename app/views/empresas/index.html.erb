<p id="notice"><%= notice %></p>

<h1>Listing Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Direccion1</th>
      <th>Direccion2</th>
      <th>Provincia</th>
      <th>Pais</th>
      <th>Zipcode</th>
      <th>Telefono</th>
      <th>Email</th>
      <th>Website</th>
      <th>Logofile</th>
      <th>Lider</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nombre %></td>
        <td><%= empresa.direccion1 %></td>
        <td><%= empresa.direccion2 %></td>
        <td><%= empresa.provincia %></td>
        <td><%= empresa.pais %></td>
        <td><%= empresa.zipcode %></td>
        <td><%= empresa.telefono %></td>
        <td><%= empresa.email %></td>
        <td><%= empresa.website %></td>
        <td><%= empresa.logofile %></td>
        <td><%= empresa.lider_id %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
